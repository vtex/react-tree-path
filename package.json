{
  "name": "react-tree-path",
  "version": "0.1.0",
  "description": "React decorator to uniquely identify components in a render tree.",
  "main": "lib/index.js",
  "scripts": {
    "watch": "babel src --out-dir=lib --watch",
    "lint": "eslint src",
    "test": "npm run lint && nyc ava src/test.js",
    "test-watch": "ava src/test.js --watch",
    "prepublish": "babel src --out-dir=lib --ignore=test.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.15.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.1.18",
    "babel-register": "^6.9.0",
    "coveralls": "^2.11.9",
    "enzyme": "^2.3.0",
    "eslint": "^2.12.0",
    "eslint-config-vtex": "^4.0.0",
    "eslint-config-vtex-react": "^1.0.2",
    "nyc": "^6.4.4",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0"
  }
}
